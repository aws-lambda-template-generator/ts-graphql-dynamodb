    "aws-sdk": "^2.844.0",
    "class-validator": "^0.13.1",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-type-json": "^0.3.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.71",
    "@types/graphql-fields": "^1.3.3",
    "@types/node": "^14.14.22",
